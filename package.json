{
  "name": "@devseed-ui/collecticons-chakra",
  "version": "0.1.0-beta",
  "description": "devseed UI Kit Collecticons for Chakra",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "serve": "parcel showcase/index.html --dist-dir dist_site -p 9000",
    "build:site": "parcel build ./showcase --dist-dir dist_site",
    "build": "yarn rollup -c ./rollup.config.mjs",
    "watch": "yarn rollup -c ./rollup.config.mjs -w",
    "clean": "rm -rf ./dist ./dist_site ./.parcel-cache",
    "ts-check": "tsc --noEmit",
    "lint": "eslint ./lib ./showcase --no-error-on-unmatched-pattern"
  },
  "engines": {
    "node": ">=18"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/developmentseed/collecticons-chakra.git"
  },
  "peerDependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "framer-motion": "^10.16.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chakra-ui/react": "^2.8.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@parcel/config-default": "^2.10.3",
    "@parcel/transformer-typescript-tsc": "^2.10.3",
    "@rollup/plugin-replace": "^5.0.5",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "clipboard": "^2.0.11",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-inclusive-language": "^2.2.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "framer-motion": "^10.16.5",
    "parcel": "^2.10.3",
    "prettier": "^3.0.3",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.5.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^5.1.6"
  },
  "keywords": [
    "css",
    "framework",
    "front-end",
    "chakra",
    "react",
    "icons",
    "collecticons"
  ]
}
